{
	"name": "web",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite --host 127.0.0.1 --port 60080 --strictPort",
		"build": "tsc -b && vite build",
		"preview": "vite preview",
		"lint": "biome check .",
		"format": "biome check --write .",
		"typecheck": "tsc -b",
		"test": "vitest run",
		"test:watch": "vitest",
		"storybook": "storybook dev -p 60081 --host 127.0.0.1",
		"build-storybook": "storybook build",
		"test-storybook": "test-storybook --url http://127.0.0.1:60081 --ci --browsers chromium --failOnConsole",
		"test:e2e": "playwright test"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.0.0",
		"@tanstack/react-router": "^1.0.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"zod": "^3.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.0.0",
		"@playwright/test": "^1.49.0",
		"@storybook/addon-essentials": "^8.0.0",
		"@storybook/addon-interactions": "^8.0.0",
		"@storybook/react-vite": "^8.0.0",
		"@storybook/test-runner": "^0.21.0",
		"@testing-library/jest-dom": "^6.0.0",
		"@testing-library/react": "^16.0.0",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"@vitejs/plugin-react": "^5.0.0",
		"autoprefixer": "^10.0.0",
		"daisyui": "^5.0.0",
		"jsdom": "^25.0.0",
		"postcss": "^8.0.0",
		"storybook": "^8.0.0",
		"tailwindcss": "^3.0.0",
		"typescript": "~5.9.0",
		"vite": "^6.0.0",
		"vitest": "^3.0.0"
	}
}
